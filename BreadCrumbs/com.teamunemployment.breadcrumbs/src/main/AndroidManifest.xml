        <activity android:name=".client.TrailDetailsViewer" android:screenOrientation="portrait"/>
        <activity android:name=".client.StoryBoard.StoryBoardActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".client.ImageChooser.TrailCoverImageSelector" android:screenOrientation="portrait"></activity>
        <activity android:name=".SaveCrumb.SaveCrumbActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".client.TestingVideo"/>
        <activity android:name=".SaveCrumb.SaveVideoActivity" android:screenOrientation="portrait"/>
